{
    "trailingSlash": true,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ],
    "redirects": [
        { "source": "/about", "destination": "/about/", "permanent": true },
        { "source": "/contact", "destination": "/contact/", "permanent": true },
        { "source": "/privacy", "destination": "/privacy/", "permanent": true },
        { "source": "/disclaimer", "destination": "/disclaimer/", "permanent": true },
        { "source": "/terms", "destination": "/terms/", "permanent": true },
        { "source": "/articles", "destination": "/articles/", "permanent": true },
        { "source": "/what-is-fixed-deposit", "destination": "/what-is-fixed-deposit/", "permanent": true },
        { "source": "/fd-vs-savings-account", "destination": "/fd-vs-savings-account/", "permanent": true },
        { "source": "/fd-interest-rates", "destination": "/fd-interest-rates/", "permanent": true }
    ],
    "rewrites": [{
        "source": "/(.*)",
        "destination": "/"
    }]
}
